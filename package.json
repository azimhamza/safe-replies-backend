{
  "name": "instagram-moderation-backend",
  "version": "1.0.0",
  "description": "Instagram comment moderation platform backend",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/src/index.js",
    "test:moderation": "tsx create-test-comments.ts && tsx test-comment-moderation.ts",
    "test:webhooks": "tsx test-webhook-simulation.ts",
    "test:suspicious-features": "tsx test-suspicious-account-features.ts",
    "cleanup:db": "tsx cleanup-database.ts",
    "migrate:run": "tsx run-migration.ts",
    "update:posts": "tsx update-posts-with-test-data.ts",
    "create:test-comments": "tsx create-test-comments.ts",
    "script:clear-db": "tsx scripts/clear-database.ts",
    "migrate": "drizzle-kit generate:pg && drizzle-kit push:pg",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "instagram",
    "moderation",
    "ai"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.490.0",
    "@aws-sdk/s3-request-presigner": "^3.490.0",
    "@react-email/components": "^1.0.7",
    "@types/node-cache": "^4.2.5",
    "autumn-js": "^0.1.75",
    "axios": "^1.6.5",
    "bcryptjs": "^2.4.3",
    "better-auth": "^1.4.15",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.41.0",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "groq-sdk": "^0.3.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "1.4.5-lts.2",
    "node-cache": "^5.1.2",
    "p-limit": "^7.2.0",
    "pg": "^8.11.3",
    "react-email": "^5.2.7",
    "resend": "^6.7.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/archiver": "^7.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.10.9",
    "@types/react": "^19.2.13",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "archiver": "^7.0.1",
    "drizzle-kit": "^0.31.4",
    "eslint": "^8.56.0",
    "react": "^19.2.4",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "packageManager": "pnpm@10.28.2+sha512.41872f037ad22f7348e3b1debbaf7e867cfd448f2726d9cf74c08f19507c31d2c8e7a11525b983febc2df640b5438dee6023ebb1f84ed43cc2d654d2bc326264",
