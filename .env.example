# Database
DATABASE_URL=postgresql://user:password@host:port/database

# Better Auth
BETTER_AUTH_SECRET=your_32_character_secret_here
BETTER_AUTH_URL=https://localhost:8080
USE_HTTPS=false
COOKIE_DOMAIN=localhost

# Resend (Email Service)
RESEND_API=re_your_resend_api_key_here
RESEND_FROM_EMAIL=Your App <noreply@yourdomain.com>

# Server
PORT=8080
FRONTEND_URL=http://localhost:3000

# Facebook API (Meta for Developers)
# Create a Facebook App at https://developers.facebook.com/apps/
FACEBOOK_APP_ID=your_facebook_app_id
FACEBOOK_APP_SECRET=your_facebook_app_secret
FACEBOOK_REDIRECT_URI=https://localhost:8080/api/facebook/oauth/callback

# Instagram Webhook
INSTAGRAM_WEBHOOK_VERIFY_TOKEN=your_webhook_verification_token

# Test Instagram tokens (for development)
TESTER=your_instagram_test_token
AZIM_TOKEN=your_instagram_token
ASTERIADUBAI_TOKEN=your_instagram_token

# Groq LLM API
GROQ_API_KEY=gsk_your_groq_api_key_here

# AWS S3 (for evidence storage)
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_REGION=us-east-1
S3_BUCKET_NAME=your-s3-bucket-name

# JINA AI (for embeddings)
JINA_API_KEY=jina_your_jina_api_key_here

# Autumn Billing API
AUTUMN_SECRET_KEY=am_sk_your_autumn_secret_key
